   <!--  Login Modal -->
    <div id="loginModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Login </h4>
            </div>
            <div class="modal-body">

            <form class="form-inline navbar-form navbar-right">
                    <input type="email" class="form-control" id="emailid" name="emailid" placeholder="Email">
                    <input type="password" class="form-control" id="password" name="passwordid" placeholder="Password">
                    <label class="checkbox" >
                        <input type="checkbox" name="approve" value="">
                        <font color="#f0f8ff">Remember me</font>
                   </label>
                    <button type="signin" class="btn btn-info btn-sm navbar-btn">Sign in</button>
                     <button type="button" class="btn btn-default btn-sm"
                      data-dismiss="modal">Cancel</button>
            </form>

            </div>
        </div>
        </div>
    </div>

   <!--  Reserve Table Modal -->
    <div id="reserveModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" id="reserveClose">&times</button>
                    <h3 class="modal-title">Reserve a Table</h3>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" role="form">
                        <div class="form-group">
                            <label class="control-label col-sm-2">Number of Guest</label>
                            <div class="btn-group col-sm-10" data-toggle="buttons">
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="1" autocomplete="off"> 1</label>
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="2" autocomplete="off"> 2</label>
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="3" autocomplete="off"> 3</label>
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="4" autocomplete="off"> 4</label>
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="5" autocomplete="off"> 5</label>
                                <label for="" class="radio-inline">
                                    <input type="radio" name="options" id="6" autocomplete="off"> 6</label>
                            </div>
                        </div>
                        <div class="form-group" role="group">
                            <label for="section" class="control-label col-sm-2" style="margin-right: 15px;">Section</label>
                            <div class="btn-group" data-toggle="buttons">
                                <label class="btn btn-success active">
                                    <input type="radio" name="options" id="nonSmoking" autocomplete="off" checked>Non Smoking
                                </label>
                                <label class="btn btn-danger">
                                    <input type="radio" name="options" id="smoking" autocomplete="off">Smoking
                                </label>
                            </div>
                        </div>
                        <div class="form-group has-feedback">
                            <label for="date" class="control-label col-sm-2">Date and Time</label>
                            <div class="col-sm-5">
                                <input type="date" class="form-control"><span class="glyphicon glyphicon-calendar form-control-feedback"></span>
                            </div>
                            <div class="col-sm-5">
                                <input type="time" class="form-control"><span class="glyphicon glyphicon-time form-control-feedback"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-8">
                                <button class="btn btn-primary">Reserve
                                </button>
                                <button type="button" class="btn btn-default" id="reserveCancel">Cancel</button>
                            </div>
                        </div>
                    </form>
                    <div class="alert alert-warning alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <strong>Warning!</strong> Please <a href="#"><strong style="color: black;">call</strong></a> us to reserve for more than six guest.
                    </div>
                </div>
            </div>
        </div>
    </div>
